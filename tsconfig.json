{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "strict": true,
        "noImplicitReturns": true,
        "noImplicitAny": true,
        "module": "es6",
        "moduleResolution": "node",
        "target": "es2015",
        "allowJs": true,
        "jsx": "react",
        "resolveJsonModule": true,
        "baseUrl": "./src"
    },
    "include": [
        "./src/**/*"
    ],
    "exclude": ["node_modules", "./src/assets/service-worker.js"]
}